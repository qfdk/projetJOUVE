{
  "store" : "jouvestore",
  "class" : "org.apache.solr.ltr.model.LinearModel",
  "name" : "jouvemodel",
  "features" : [
    { "name" : "isInStock" },
    { "name" : "isInPromo" },
    { "name" : "originalScore" }
  ],
  "params" : {
    "weights" : {
      "isInStock" : 0.6,
      "isInPromo" : 1.0,
      "originalScore" : 0.1
    }
  }
}